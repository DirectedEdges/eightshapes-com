{
  "name": "eightshapes_com",
  "version": "2.0.0",
  "description": "Marketing site",
  "main": "index.html",
  "scripts": {
    "start": "npm-run-all build --parallel start:11ty watch:concatenate-component-macros watch:styles",
    "start:11ty": "npx @11ty/eleventy --serve",
    "start:11ty-debug": "DEBUG=Eleventy* npx @11ty/eleventy --serve",
    "build": "npm-run-all clean:site concatenate-component-macros build:11ty build:styles",
    "build:11ty": "npx @11ty/eleventy",
    "build:styles": "node build-tasks/compile-sass.js",
    "clean:site": "npx trash docs/*",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate-articles-to-md": "node build-tasks/article-njk-to-md-migration.js",
    "concatenate-component-macros": "node build-tasks/concatenate-component-macros.js",
    "watch:concatenate-component-macros": "chokidar \"src/_components/**/*.njk\" -c \"npm run concatenate-component-macros\"",
    "watch:styles": "chokidar \"src/_styles/**/*.scss\" -c \"npm run build:styles\"",
    "prepack": "gulp build:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://nathanacurtis@github.com/EightShapes/eightshapes_com.git"
  },
  "authors": "Nathan Curtis, Dan Brown, Chris Detzi, James Melzer, Kevin Powell",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EightShapes/eightshapes_com/issues"
  },
  "homepage": "http://eightshapes.com",
  "devDependencies": {
    "@11ty/eleventy": "^0.10.0",
    "autoprefixer": "^9.8.0",
    "chokidar-cli": "^2.1.0",
    "del": "^3.0.0",
    "eleventy-plugin-date": "^1.0.0",
    "eleventy-plugin-reading-time": "0.0.1",
    "esds-build": "^0.51.0",
    "fs": "0.0.1-security",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "marked": "^0.8.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.32",
    "prettier": "^2.0.5",
    "sass": "^1.26.8",
    "strip-indent": "^3.0.0",
    "stylelint": "^13.6.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "stylelint-prettier": "^1.1.2",
    "trash-cli": "^3.0.0"
  },
  "dependencies": {
    "@eightshapes/esds-code-snippet": "^0.10.2",
    "@webcomponents/webcomponentsjs": "^2.4.1",
    "esds-library": "^0.11.0"
  }
}
